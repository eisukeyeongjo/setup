---

devs:
  vars:
    groupname: GROUPNAME
    username: USERNAME
    password: PASSWORD
    mysql_root_password: MYSQL_ROOT_PASSWORD
    mysql_username: MYSQL_USERNAME
    mysql_password: MYSQL_PASSWORD
  hosts:
    dev_ubuntu:
      ansible_host: TARGET_HOSTNAME
      ansible_user: TARGET_USERNAME
      ansible_ssh_private_key_file: "~/path/to/key.pem"
